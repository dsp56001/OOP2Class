<Window x:Class="WpfAppDog.WpfMammal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppDog"
        xmlns:views="clr-namespace:WpfAppDog.Views"
        mc:Ignorable="d"
        Title="WpfDog" Height="600" Width="300">
    <Grid>
        <StackPanel>
            <views:MammalView x:Name="MammalView1" Loaded="MammalViewControl_Loaded"/>
            <StackPanel>
                <Button x:Name="btnNext" Click="btnNext_Click" >Next</Button>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
